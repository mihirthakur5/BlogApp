{
    "language_servers": ["!eslint"],
    "lsp": {
        "tailwindcss-language-server": {
            "settings": {
                "classFunctions": ["cva", "cx"],
                "experimental": {
                    "classRegex": ["[cls|className]\\s\\:\\=\\s\"([^\"]*)"],
                },
            },
        },
    },
    "plugins": ["prettier-plugin-tailwindcss"],
}
